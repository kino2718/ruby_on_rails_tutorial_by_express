{
  "name": "ruby_on_rails_tutorial_by_express",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "author": "kino2718",
  "type": "commonjs",
  "scripts": {
    "start": "node app/server.js",
    "dev": "NODE_ENV=development DEBUG=express:* nodemon app/server.js",
    "start:watch": "nodemon app/server.js",
    "test": "knex migrate:latest --env test && NODE_ENV=test jest --runInBand; rm -f ./db/test.sqlite3",
    "test:watch": "knex migrate:latest --env test && NODE_ENV=test jest --watch --runInBand; rm -f ./db/test.sqlite3",
    "build:css": "sass --load-path=node_modules app/assets/stylesheets/application.scss app/assets/stylesheets/application.css --style=compressed",
    "watch:css": "sass --watch --load-path=node_modules app/assets/stylesheets/application.scss:app/assets/stylesheets/application.css --style=compressed",
    "migrate:latest:dev": "knex migrate:latest --env development",
    "migrate:rollback:dev": "knex migrate:rollback --env development",
    "migrate:reset:dev": "knex migrate:rollback --all --env development && npx knex migrate:latest --env development",
    "migrate:latest:prod": "knex migrate:latest --env production",
    "migrate:rollback:prod": "knex migrate:rollback --env production",
    "console": "NODE_ENV=development node -i -r ./setup_console.js",
    "console:sandbox": "SQLITE_FILENAME=./db/tmp.sqlite3 knex migrate:latest --env development && NODE_ENV=development SQLITE_FILENAME=./db/tmp.sqlite3 node -i -r ./setup_console.js; rm -f ./db/tmp.sqlite3",
    "lint": "eslint .",
    "lint:watch": "nodemon --ext js,mjs,cjs --exec \"eslint .\""
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "bcryptjs": "^3.0.2",
    "bootstrap": "^5.3.8",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.7",
    "csrf": "^3.1.0",
    "dotenv": "^17.2.2",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "sass": "^1.92.1",
    "sqlite3": "^5.1.7",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "cheerio": "^1.1.2",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  }
}
