<%- include('../layouts/header') -%>

<h1>All users</h1>

<%- paginate(pagination, '/users') %>

<ul class="users">
    <% users.forEach((user) => { %>
        <li>
            <%- gravatarFor(user, {size: 50}) %>
            <a href="/users/<%= user.id %>"><%= user.name %></a>
        </li>
    <% }) %>
</ul>

<%- paginate(pagination, '/users') %>

<%- include('../layouts/footer') -%>
